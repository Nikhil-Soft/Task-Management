<div id="<%= dom_id project %>">
  <p>
    <strong>Title:</strong>
    <%= project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= project.status %>
  </p>

  <p>
    <strong>Client:</strong>
    <%= project&.title %>
  </p>

  <p>
    <strong>User:</strong>
    <%= project&.user&.email %>
  </p>

</div>
